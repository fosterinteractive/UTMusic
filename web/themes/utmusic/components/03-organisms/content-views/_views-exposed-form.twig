{#
/**
 * @file
 * Theme override for a 'form' element.
 *
 * Available variables
 * - attributes: A list of HTML attributes for the wrapper element.
 * - children: The child elements of the form.
 *
 * @see template_preprocess_form()
 */
#}

{% set additional_class = 'm-exposed-form__wrapper' %}
  

<div class='{{ additional_class }} {{ main_layout_classes }}'>
  <form{{ attributes.addClass( layout_variation ) }}>
    {{ children }}
  </form>
</div>

{{attach_library('utmusic/announcement')}}